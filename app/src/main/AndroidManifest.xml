<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.avijit.healthcareforcovid19">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_covid"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_covid_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".CovidInfoActivity"
            android:theme="@style/Theme.MaterialComponents.NoActionBar"/>
        <activity android:name=".FamilyInfo" />
        <activity android:name=".Profile" />
        <activity android:name=".CovidTest" />
        <activity android:name=".Index" />
        <activity android:name=".SignUpUi" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>